{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/swiper-block",
  "version": "0.1.0",
  "title": "Swiper Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "enableNavigation": {
      "type": "boolean"
    },
    "enablePagination": {
      "type": "boolean"
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "slides": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "content": {
            "type": "string"
          },
          "image": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string"
              },
              "alt": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "swiper-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}